#include <stdio.h>
int main() {
    int i,n,num,sum=0;
    printf("Enter the number of elements: ");
    scanf("%d",&n);
    for(i=1;i<=n;i++) {
        printf("\nEnter the number: ");
        scanf("%d",&num);
        sum=sum+num;
    }
    printf("\nThe sum of all elements is: %d",sum);
    return 0;
}
