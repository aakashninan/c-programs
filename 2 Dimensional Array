#include <stdio.h>
int main() {
    int row,columns,i,j,a[30][30];
    printf("Enter the number of rows : ");
    scanf("%d",&row);
    printf("Enter the number of columns : ");
    scanf("%d",&columns);
    for(i=0;i<row;i++) {
        for(j=0;j<columns;j++) {
            printf("Enter the value of a[%d][%d] : ",i,j);
            scanf("%d",&a[i][j]);

        }
    }
    for(i=0;i<row;i++) {
        printf("\n");
        for(j=0;j<columns;j++) {
            printf("a[%d][%d]=%d\t",i,j,a[i][j]);

        }
    }
