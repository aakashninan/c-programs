#include <stdio.h>

int main() {
    int num1,num2;
    printf("Enter 2 numbers\n");
    scanf("%d %d",&num1,&num2);
    printf("First number is %d\n",num1);
    printf("Second number is %d\n",num2);
    num1=num1^num2;
    num2=num1^num2;
    num1=num1^num2;
    printf("First number = %d",num1);
    printf("\nSecond number = %d",num2);
    return 0;
}
