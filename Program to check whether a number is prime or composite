#include <stdio.h>
int main() {
    int num,half,i;
    printf("Enter a number : ");
    scanf("%d",&num);
    half=num/2;
    if(num==1) {
        printf("%d is neither a Prime Number nor a Composite Number",num);
        return 0;
    }
    for(i=2;i<=half;i++)
    {


        if (num%i==0)
        {
            printf("%d is a not a Prime Number \n",num);
            return 0;;
        }
    }

    printf("%d is a Prime Number \n",num);

    return 0;
}


