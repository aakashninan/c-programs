#include <stdio.h>
int main() {
    int a[20],largest,i,n;
    printf("Enter the number of numnbers ");
    scanf("%d",&n);
    for(i=0;i<n;i++) {
        printf("Enter the elemt in a[%d]",i);
        scanf("%d",&a[i]);
    }
    largest=a[0];
    for(i=0;i<n;i++) {
        if(a[i]>largest) {
            largest=a[i];
        }

    }
    printf("The largest number is %d",largest);
    return 0;
}
